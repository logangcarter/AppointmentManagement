<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="479.0" prefWidth="1076.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.CustomerDirectoryController">
   <children>
      <Label layoutX="39.0" layoutY="33.0" text="Customer Directory">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <TableView fx:id="customerTableView" layoutX="41.0" layoutY="75.0" prefHeight="330.0" prefWidth="981.0">
         <columns>
            <TableColumn fx:id="idCol" prefWidth="55.0" text="ID" />
            <TableColumn fx:id="nameCol" prefWidth="191.0" text="Name" />
            <TableColumn fx:id="phoneCol" prefWidth="188.0" text="Phone Number" />
            <TableColumn fx:id="addressCol" prefWidth="214.0" text="Address" />
            <TableColumn fx:id="countryCol" prefWidth="170.0" text="Country" />
            <TableColumn fx:id="regionCol" prefWidth="162.0" text="Region" />
         </columns>
      </TableView>
      <Button fx:id="appointmentCalendarButton" layoutX="821.0" layoutY="33.0" mnemonicParsing="false" onAction="#onActionAppointmentCalendar" text="Appointment Calendar" />
      <Button fx:id="addCustomerButton" layoutX="732.0" layoutY="426.0" mnemonicParsing="false" onAction="#onActionAddCustomer" text="Add" />
      <Button fx:id="modifyCustomerButton" layoutX="785.0" layoutY="426.0" mnemonicParsing="false" onAction="#onActionModifyCustomer" text="Modify" />
      <Button fx:id="deleteCustomerButton" layoutX="906.0" layoutY="426.0" mnemonicParsing="false" onAction="#onActionDeleteCustomer" text="Delete" />
   </children>
</AnchorPane>
